{
  "name": "afromarket_backend",
  "version": "1.0.0",
  "description": "backend service for afromarket",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon index.js",
    "db:init": "sequelize-cli db:create",
    "add-migration": "makemigration --name",
    "new-migration": "makemigration --preview",
    "migration-0": "runmigration",
    "migration": "sequelize-cli db:migrate",
    "seed-cat": "sequelize-cli db:seed --seed category.seed.js",
    "create-db": "sequelize-cli db:create",
    "drop-db": "sequelize-cli db:drop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/preciousemmanuel/afromarket_backend.git"
  },
