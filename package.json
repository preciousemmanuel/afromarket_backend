  },
  "homepage": "https://github.com/preciousemmanuel/afromarket_backend#readme",
  "dependencies": {
    "@joi/date": "^2.1.0",
    "axios": "^1.1.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cloudinary": "^1.32.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.2",
    "email-validator": "^2.0.4",
    "express": "^4.18.1",
    "express-sslify": "^1.2.0",
    "flutterwave-node-v3": "^1.0.11",
